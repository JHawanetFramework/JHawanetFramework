<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <center>
                <TabPane fx:id="tabPane" prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE">
                    <tabs>
                        <Tab fx:id="indicatorTab" text="Choose Indicators and Experiments">
                            <content>
                                <HBox prefHeight="100.0" prefWidth="200.0">
                                    <padding>
                                        <Insets top="10.0" />
                                    </padding>
                                    <VBox spacing="5.0" HBox.hgrow="ALWAYS">
                                        <HBox.margin>
                                            <Insets />
                                        </HBox.margin>
                                        <Label text="Choose Indicators:">
                                            <VBox.margin>
                                                <Insets left="10.0" />
                                            </VBox.margin>
                                        </Label>
                                        <AnchorPane fx:id="indicatorPane" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
                                    </VBox>
                                </HBox>
                            </content>
                        </Tab>
                        <Tab fx:id="chooseExperimentsTab" disable="true" text="Choose Experiments" />
                        <Tab fx:id="configurationTab" disable="true" text="Configure Experiments" />
                    </tabs>
                </TabPane>
            </center>
            <bottom>
                <HBox alignment="CENTER_RIGHT" spacing="10.0">
                    <children>
                        <Button fx:id="previousButton" mnemonicParsing="false" text="Previous" disable="true"/>
                        <Button fx:id="nextRunButton" mnemonicParsing="false" prefWidth="61.0" text="Next" />
                    </children>
                    <BorderPane.margin>
                        <Insets />
                    </BorderPane.margin>
                    <padding>
                        <Insets bottom="10.0" right="10.0" top="10.0" />
                    </padding>
                </HBox>
            </bottom>
        </BorderPane>
    </children>
</AnchorPane>
