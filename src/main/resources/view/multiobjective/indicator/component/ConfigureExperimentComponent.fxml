<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.*?>
<fx:root  prefWidth="600" prefHeight="400"  type="javafx.scene.layout.StackPane"
         xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <SplitPane fx:id="configurationSplitPane" dividerPositions="0.29797979797979796">
        <items>
            <AnchorPane maxWidth="${configurationSplitPane.width * 0.30}" minHeight="0.0" minWidth="0.0" prefHeight="160.0"
                        prefWidth="100.0">
                <children>
                    <TreeView fx:id="experimentTreeView" layoutX="-12.0" layoutY="86.0" prefHeight="200.0" prefWidth="200.0"
                              AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                              AnchorPane.topAnchor="0.0"/>
                </children>
            </AnchorPane>
            <ScrollPane fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER" prefHeight="200.0" prefWidth="200.0"
                        vbarPolicy="NEVER">
                <content>
                    <StackPane fx:id="configurationPane" prefHeight="200.0" prefWidth="200.0"/>
                </content>
            </ScrollPane>
        </items>
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </padding>
    </SplitPane>
</fx:root>
