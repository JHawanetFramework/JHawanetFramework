<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<fx:root prefHeight="400.0" prefWidth="600.0" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <spacing>10</spacing>
    <AnchorPane VBox.vgrow="ALWAYS">
        <children>
            <TableView fx:id="problemTableView" editable="true" layoutX="90.0" layoutY="2.0" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                <columns>
                    <TableColumn editable="false" prefWidth="75.0" sortable="false" text="Problem Name" />
                    <TableColumn prefWidth="75.0" sortable="false" text="Number of instances" />
                </columns>
                <columnResizePolicy>
                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                </columnResizePolicy>
            </TableView>
        </children>
    </AnchorPane>
    <AnchorPane VBox.vgrow="ALWAYS">
        <children>
            <TableView fx:id="algorithmTableView" editable="true" layoutX="145.0" layoutY="-17.0" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                <columns>
                    <TableColumn editable="false" prefWidth="75.0" sortable="false" text="Algorithm Name" />
                    <TableColumn prefWidth="75.0" sortable="false" text="Number of instances" />
                </columns>
                <columnResizePolicy>
                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                </columnResizePolicy>
            </TableView>
        </children>
    </AnchorPane>
    <padding>
        <Insets left="10" right="10" top="10" />
    </padding>
</fx:root>
